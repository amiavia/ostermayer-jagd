{
  "expo": {
    "name": "Ballistik Rechner",
    "slug": "ballistik-rechner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ballistik",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D1F0D"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ostermayer.ballistik"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0D1F0D"
      },
      "package": "com.ostermayer.ballistik"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
